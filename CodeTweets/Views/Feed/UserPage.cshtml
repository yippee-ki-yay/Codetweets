@model IEnumerable<CodeTweets.Models.CodePost>

<div class="row">
    <div class="col-lg-8">
        @if (ViewBag.banned == false)
        {
         <h2>Profile: @ViewBag.username see all of his posts</h2>
        }
        else
        {
            <h2>User has you blocked</h2>
        }
    </div>

    <div class="col-lg-4">
        <button class="btn btn-default">Follow</button>
    </div>
</div>

@if(ViewBag.banned == false)
{
foreach (var item in Model)
{
    <div class="panel panel-info">
        <div class="panel-heading">
            @Html.DisplayFor(modelItem => item.title)
        </div>
        <div class="panel-body">
            item.content

            <div class="pull-right comBtn">
                <button class="btn btn-default cmnt" id="@item.id">Baci comment</button>
            </div>

        </div>
    </div>
    }
}
